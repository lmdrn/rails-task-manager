<h1>Edit Task</h1>

<div class=form>
<%= form_for(@task) do |f| %>
  <div class="form-group">
    <%= f.label :task %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :infos %>
    <%= f.text_field :details, class: 'form-control' %>
  </div>

  <%= f.submit class: 'btn btn-primary' %>
<% end %>
</div>
